{
  "name": "lukeshiru-card",
  "description": "Simple CLI presentation card",
  "version": "1.0.7",
  "author": {
    "name": "Lucas Ciruzzi",
    "email": "hi@lshi.ru",
    "url": "https://lshi.ru"
  },
  "bin": "./dist/index.js",
  "bugs": {
    "url": "https://github.com/lukeshiru/lukeshiru-card/issues"
  },
  "dependencies": {
    "@vangware/forcli": "^1.3.0",
    "boxen": "^5.0.1",
    "clear": "^0.1.0"
  },
  "devDependencies": {
    "@types/clear": "^0.1.1",
    "@types/eslint": "^7.2.13",
    "@types/node": "^15.12.1",
    "@types/prettier": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vangware/base": "^4.7.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-ban": "^1.5.2",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "4.3.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://lshi.ru",
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean lint pre-compile"
    }
  },
  "keywords": [
    "card",
    "presentation"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lukeshiru/lukeshiru-card.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "tsc",
    "lint": "eslint src/**/*.ts",
    "pre-compile": "tsc --noemit --project tsconfig.json",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s compile prettify",
    "prettify": "prettier --write './dist/**/*.{js,ts}'",
    "start": "ts-node ./src/index.ts"
  },
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts"
}
