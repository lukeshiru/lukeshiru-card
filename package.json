    "url": "https://github.com/lukeshiru/lukeshiru-card/issues"
  },
  "dependencies": {
    "@vangware/forcli": "^1.1.16",
    "boxen": "^4.2.0",
    "clear": "^0.1.0"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/eslint": "^7.2.5",
    "@types/node": "^14.14.10",
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@vangware/base": "^4.1.6",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-ban": "^1.5.1",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://lshi.ru",
  "husky": {
    "hooks": {
      "pre-commit": "run-s clean lint pre-compile"
    }
  },
  "keywords": [
    "card",
    "presentation"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lukeshiru/lukeshiru-card.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "tsc",
    "lint": "eslint src/**/*.ts",
    "pre-compile": "tsc --noemit --project tsconfig.json",
    "prepublishOnly": "run-s compile prettify",
    "prettify": "prettier --write './dist/**/*.{js,ts}'",
    "start": "ts-node ./src/index.ts"
  },
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts"
